{
	"compilerOptions": {
		"noEmit": true,
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"useDefineForClassFields": true,
		"skipLibCheck": true,
		"pretty": true,
		"moduleResolution": "node"
	},
	"extends": "@astolat/ts-config",
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"**/*.js",
		"**/*.jsx"
	]
}